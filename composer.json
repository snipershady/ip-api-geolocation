{
    "name": "snipershady/ipapigeolocation",
    "description": "Free wrapper to easily implements ip-api.com free API",
    "keywords": ["ip-api", "geolocation", "ip-api.com"],
    "homepage": "https://www.spinfo.it",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Stefano Perrini",
            "email": "perrini.stefano@gmail.com",
            "homepage": "https://www.spinfo.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "IPApiGeolocationWrapper\\": "src/"
        }       
    },
    "autoload-dev": {
        "psr-4": {
            "IPApiGeolocationWrapper\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8"
    }
}
